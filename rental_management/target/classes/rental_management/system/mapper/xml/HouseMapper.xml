<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="rental_management.system.mapper.HouseMapper">
	<resultMap type="rental_management.system.bean.House" id="house">
		<id property="id" column="id" />
		<result property="houseId" column="house_id" />
		<result property="address" column="address" />
	</resultMap>
	<select id="selectHouseId" resultMap="house">
		select id,house_id,address from gd_house_info where house_id=#{houseId}
	</select>
	<select id="selectAll" resultMap="house">
		select id,house_id,address from gd_house_info
	</select>

	<insert id="insert" parameterType="rental_management.system.bean.House">
		insert into
		gd_house_info(house_id,address) values(
		#{houseId,jdbcType=NUMERIC},
		#{address,jdbcType=VARCHAR}
		);
	</insert>
	<delete id="delete" parameterType="int">
		delete from gd_house_info where house_id=#{houseId}
	</delete>

</mapper>