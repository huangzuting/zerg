<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="rental_management.system.mapper.UserPropertyMapper">
	<insert id="insert" >
		insert into
		gd_user_address(user_id,address_id) values(
		#{userId,jdbcType=NUMERIC},
		#{addressId,jdbcType=NUMERIC}
		);
	</insert>
	<delete id="deleteuser" parameterType="int">
		delete from gd_user_address where
		address_id=#{addressId}
	</delete>
	<delete id="deleteaddress" parameterType="int">
		delete from gd_user_address where
		user_id=#{userId}
	</delete>
	<!-- 更新地址所属人 -->
	<update id="updateuser" >
		update gd_user_address set
		user_id=#{userId,jdbcType=NUMERIC}
		where address_id=#{addressId}
	</update>
	<!-- 更新人所属人地址 -->
	<update id="updateaddress" >
		update gd_user_address set
		address_id=#{addressId,jdbcType=NUMERIC}
		where user_id=#{userId}
	</update>
	<select id="selectuser" resultType="int">
		select address_id from
		gd_user_address where user_id=#{userId}
	</select>
	<select id="selectaddress" resultType="int">
		select user_id from
		gd_user_address where address_id=#{addressId}
	</select>
</mapper>